# LiteLLM Keys (generate with: openssl rand -hex 32)
LITELLM_MASTER_KEY=sk-...
LITELLM_SALT_KEY=sk-...

# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-secure-password-here
LITELLM_DB_PASSWORD=your-secure-password-here
OPENWEBUI_DB_PASSWORD=your-secure-password-here

# Domain Configuration (production only)
OPENWEBUI_DOMAIN=your-domain.com
LITELLM_DOMAIN=api.your-domain.com

# SearXNG Secret Key (generate with: openssl rand -hex 32)
SEARXNG_SECRET=your-secret-key-here
